---
import AppProfilePage from '../../../../templates/AppProfilePage.astro';
import { getAppProfileStaticPaths, type AppProfile } from '../../../../i18n/app-profiles';

export function getStaticPaths() {
  return getAppProfileStaticPaths('en');
}

interface Props {
  profile: AppProfile;
}

const { profile } = Astro.props;
---

<AppProfilePage locale="en" path={Astro.url.pathname} profile={profile} />
